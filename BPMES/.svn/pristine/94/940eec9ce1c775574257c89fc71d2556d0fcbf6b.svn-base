    <!-- 查询半成品出入库信息-->
	<select id="queryBCPProduct" parameterType="hashmap" resultType="java.util.HashMap"> 	   
	   <include refid="oraclePagingFileTop"></include> 		
        DD.* FROM (SELECT T1.SEMI_PRO_CD,T1.SEMI_PRO_NAME,T1.STATUS,T1.PRO_NUM,T1.PRO_SHELF_ID,T5.STORE_NAME AS PRO_SHELF_NAME,T1.PRO_POSITION,
		CASE
		  WHEN T1.PRO_POSITION=1 THEN '生产库位'
		  WHEN T1.PRO_POSITION=2 THEN '注塑库位' 
		  WHEN T1.PRO_POSITION=3 THEN '回流炉库位'
		  WHEN T1.PRO_POSITION=4 THEN '车间库位'
		  END AS PRO_POSITION_NAME,
		T1.CRT_ID,T1.UPT_ID,TO_CHAR(T1.CRT_DT,'YYYY-MM-DD HH24:MI:SS') AS CRT_DT,TO_CHAR(T1.UPT_DT,'YYYY-MM-DD HH24:MI:SS') AS UPT_DT
       	FROM WMS1.R_WMS_SEMI_PRODUCT T1 LEFT JOIN  WMS1.C_WMS_STORE_T T5 ON T1.PRO_SHELF_ID=T5.STORE_ID
       	LEFT JOIN (SELECT T2.USE_CD,T3.EMP_NM FROM iplant1.C_IPLANT_U1_T T2 LEFT JOIN iplant1.C_IPLANT_E1_T T3 ON T2.EMP_NO = T3.EMP_CD) T4 ON T1.CRT_ID=T4.USE_CD
       	<where> 1=1			
			<if test="SEMI_PRO_CD !=null and SEMI_PRO_CD !='' "> AND T1.SEMI_PRO_CD = '${SEMI_PRO_CD}'</if>
			<if test="SEMI_PRO_NAME !=null and SEMI_PRO_NAME !='' "> AND T1.SEMI_PRO_NAME LIKE '%${SEMI_PRO_NAME}%'</if>
			<if test="PRO_SHELF_ID !=null and PRO_SHELF_ID !='' "> AND T1.PRO_SHELF_ID='${PRO_SHELF_ID}'</if>
			<if test="STATUS !=null and STATUS !='' "> AND T1.STATUS='${STATUS}'</if>
		</where> ORDER BY T1.CRT_DT DESC)DD
       <include refid="oraclePagingFileBottom"></include> 	       					
	</select>
