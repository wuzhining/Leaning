		LEFT JOIN WMS1.WMS_ORDER C ON A.ORDER_ID = C.ORDER_ID LEFT JOIN IPLANT1.C_IPLANT_DD_T D ON A.CHECKIN_STATUS=D.DICT_IT
		<where>	1=1	  	
		  	<if test="ORDER_ID !=null and ORDER_ID != ''"> and A.ORDER_ID='${ORDER_ID}'</if>			
		  	<if test="BARCODE !=null and BARCODE != ''"> and A.BARCODE LIKE '%${BARCODE}%'</if>
