	 	         </if>
	 	         <if test="MATERIA_ID !=null and MATERIA_ID !='' ">
	 	       	    AND T2.MATERIA_ID LIKE '%${MATERIA_ID}%'	 
	 	         </if>
	 	         <if test="ITEM_NM !=null and ITEM_NM !='' ">
