			     TO_CHAR(T2.CREATE_DATE,'YYYY-MM-DD HH24:MI:SS') AS CREATE_DATE,
			     T2.SUPPLIER_ID,
			     T2.SUPPLIER_NAME   AS SUP_NM,
			     T2.CONNECTOR_NAME  AS CT_MN,
			     T2.CONNECTOR_PHONE AS PN_NB,
