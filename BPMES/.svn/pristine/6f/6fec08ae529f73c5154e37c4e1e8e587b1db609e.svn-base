    <div id="enditTab" class="easyui-dialog" style="width: 420px;height: 480px;" closed="true" data-options="modal: true,
				buttons: [{
					text:'保存',
					iconCls:'icon-ok',
					handler:function(){
						saveFuncMes(); 
					}
				},{
					text:'取消',
